import numpy as np

S = 1
G = 6
K = 1

A = np.array([[1, 5, 1],
              [4, -1, 1],
              [S, G, 2*(S+G+K)]])

B = np.array([S-G+K+10, S-G+4*K-2, 2*((S-G)*(S+G+K)+G)+K*S])

X = np.linalg.solve(A, B)

print("Решение системы уравнений:")
print("x1 =", X[0])
print("x2 =", X[1])
print("x3 =", X[2])
